{
  "name": "bluffler/pokercore",
  "description": "PHP Poker Core library",
  "keywords": [
    "poker",
    "texas holdem",
    "holdem",
    "holdem library",
    "hand",
    "board",
    "card",
    "cards"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ilya Gusev",
      "email": "mail@igusev.ru"
    }
  ],
  "require": {
    "php": "^7.2",
    "drupol/phpermutations": "^1.3",
    "nesbot/carbon": "^2.24"
  },
  "require-dev": {
    "phpunit/phpunit": "8.*",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "PokerCore\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PokerCore\\Tests\\": "tests/"
    }
  }
}
